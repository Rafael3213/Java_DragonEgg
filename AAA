# Navigate to your datapack folder
cd path/to/minecraft-dragon-egg-transfer

# Initialize a new git repository
git init

# Add all files
git add .

# Commit your changes
git commit -m "Initial commit for dragon egg transfer datapack"

# Link to your GitHub repository
git remote add origin https://github.com/yourusername/minecraft-dragon-egg-transfer.git

# Push the changes to GitHub
git branch -M main
git push -u origin main
